{
  "buildCommand": "cd Wedding-planner/client && npm install && npm run build",
  "outputDirectory": "Wedding-planner/client/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://wedding-planner-wj86.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
